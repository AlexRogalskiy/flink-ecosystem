{
  "name": "flink-ecosystem",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "esm": "^3.2.25",
    "kcors": "^2.2.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-file-router": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react": "^16.8.6",
    "react-animate-height": "^2.0.9",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "client": "react-scripts start",
    "nodemon": "nodemon -r esm src/server/main.js --ignore src/client",
    "server": "node -r esm src/server/main.js",
    "start": "concurrently --kill-others-on-fail --prefix '[{name}]' --names 'client,server' 'npm run client' 'npm run nodemon'",
    "test": "react-scripts test"
  }
}
